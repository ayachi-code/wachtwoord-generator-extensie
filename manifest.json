{
  "manifest_version": 2,
  "name": "Wachtwoord generator",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js":["content.js"]
    }
  ],

  "browser_action": {
    "default_title": "Wachtwoord generator",
    "default_popup": "/public/index.html",
    "default_icon": "/public/extra/foto.png"
  }


}
